name:                Agda-LLS
version:             0.0.1.0
-- synopsis:            
-- description:         
homepage:            https://github.com/heades/Agda-LLS
license:             BSD3
license-file:        LICENSE
author:              Dr. Harley Eades, et al.
maintainer:          Heades@gmail.com
-- copyright:           
category:            Math
build-type:          Simple
extra-source-files:  ChangeLog.md
cabal-version:       >=1.10

executable Agda-LLS
  main-is:             Main.hs
